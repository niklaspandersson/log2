{
  "name": "log2",
  "version": "1.0.0",
  "description": "A logbook SPA that tries to simplify logging so much that it actually gets done",
  "main": "index.js",
  "scripts": {
    "test": "json-server --watch ./test/data/db.json --static ./dist/public_html --port 8000 &",
    "build": "webpack",
    "build-backend": "tsc -p tsconfig-backend.json",
    "serve": "node dist/backend/backend/main.js",
    "css-build": "node-sass  --source-map true src/sass/main.scss dist/public_html/css/main.css",
    "watch-css": "node-sass  --source-map true --watch src/sass/main.scss dist/public_html/css/main.css"
  },
  "author": "Niklas P Andersson",
  "license": "ISC",
  "devDependencies": {
    "@types/classnames": "^2.2.7",
    "@types/moment": "^2.13.0",
    "@types/react": "^16.8.1",
    "@types/react-dom": "^16.0.11",
    "@types/react-motion": "0.0.27",
    "json-server": "^0.14.2",
    "node-sass": "^4.11.0",
    "sass-loader": "^7.1.0",
    "source-map-loader": "^0.2.4",
    "ts-loader": "^5.3.3",
    "tslint": "^5.12.1",
    "typescript": "^3.3.1",
    "webpack": "^4.29.1",
    "webpack-cli": "^3.2.3",
    "@types/argparse": "^1.0.35",
    "@types/express": "^4.16.0",
    "@types/mongodb": "^3.1.12"
  },
  "dependencies": {
    "bulma": "^0.7.2",
    "classnames": "^2.2.6",
    "moment": "^2.24.0",
    "react": "^16.8.0",
    "react-dom": "^16.8.0",
    "react-motion": "^0.5.2",
    "argparse": "^1.0.10",
    "express": "^4.16.3",
    "mongodb": "^3.1.7"
  }
}
